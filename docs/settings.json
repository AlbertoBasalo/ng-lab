{
  "editor.accessibilitySupport": "off",
  "editor.autoClosingBrackets": "always",
  "editor.autoIndent": "full",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.addMissingImports": "explicit",
    "source.fixAll": "explicit",
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.detectIndentation": false,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.glyphMargin": false,
  "editor.guides.bracketPairs": true,
  "editor.hover.above": false,
  "editor.hover.sticky": true,
  "editor.inlineSuggest.enabled": true,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.parameterHints.enabled": false,
  "editor.quickSuggestions": {
    "comments": "off",
    "other": "inline",
    "strings": "off"
  },
  "editor.quickSuggestionsDelay": 1000,
  "editor.renderWhitespace": "none",
  "editor.semanticHighlighting.enabled": false,
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 120,
  "emmet.triggerExpansionOnTab": true,
  "eslint.enable": true,
  "eslint.run": "onType",
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/node_modules/": true,
    "node_modules/": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "dist/": true
  },
  "html.format.enable": false,
  "html.format.indentInnerHtml": true,
  "html.format.wrapAttributes": "auto",
  "html.format.wrapLineLength": 120,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "material-icon-theme.files.associations": {
    "*.config.ts": "tune",
    "*.config.server.ts": "tune",
    "*.card.ts": "angular",
    "*.class.ts": "raml",
    "*.component.ts": "angular",
    "*.dialog.ts": "angular",
    "*.dto.ts": "raml",
    "*.effect.ts": "redux-action",
    "*.entity.ts": "raml",
    "*.enum.ts": "tune",
    "*.environment.ts": "tune",
    "*.form.ts": "angular",
    "*.functions.ts": "purescript",
    "*.index.ts": "lib",
    "*.interface.ts": "raml",
    "*.interceptor.ts": "angular-guard",
    "*.list.ts": "angular",
    "*.page.ts": "angular-resolver",
    "*.port.ts": "capacitor",
    "*.provider.ts": "tune",
    "*.repository.ts": "database",
    "*.router.ts": "routing",
    "*.service.ts": "angular-service",
    "*.settings.ts": "tune",
    "*.signal.ts": "redux-selector",
    "*.state.ts": "redux-selector",
    "*.store.ts": "redux-store",
    "*.template.ts": "angular",
    "*.type.ts": "raml",
    "*.use-case.ts": "purescript",
    "*.utils.ts": "purescript",
    "*.validation.ts": "document",
    "*.widget.ts": "angular",
    "CLI.md": "console",
    "TASKS.md": "todo"
  },
  "material-icon-theme.folders.associations": {
    "domain": "core",
    "shell": "stack",
    "state": "redux-store",
    "ui": "views"
  },
  "prettier.bracketSameLine": true,
  "prettier.htmlWhitespaceSensitivity": "ignore",
  "prettier.printWidth": 120,
  "prettier.requireConfig": false,
  "search.exclude": {
    "**/env": true,
    "node_modules/": true
  },
  "totalTypeScript.hideAllTips": true,
  "totalTypeScript.hideBasicTips": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.tree.renderIndentGuides": "always"
}
