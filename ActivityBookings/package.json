{
  "name": "activity-bookings",
  "version": "17.1.2",
  "description": "Booking and management of activities",
  "author": {
    "name": "Alberto Basalo",
    "url": "https://albertobasalo.dev"
  },
  "scripts": {
    "ng": "ng",
    "chrome": "start chrome -incognito -auto-open-devtools-for-tabs http://localhost:4200",
    "start": "npm run chrome && ng serve --force-esbuild",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:ActivityBookings": "node dist/activity-bookings/server/server.mjs",
    "api": "json-server-auth ../db/prod/d.json -r ../db/r.json",
    "api:reset": "copyfiles -f ../db/reset/d.json ../db/prod && npm run api",
    "api:seed": "copyfiles -f ../db/seed/d.json ../db/prod && npm run api"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.0",
    "@angular/common": "^17.1.0",
    "@angular/compiler": "^17.1.0",
    "@angular/core": "^17.1.0",
    "@angular/forms": "^17.1.0",
    "@angular/platform-browser": "^17.1.0",
    "@angular/platform-browser-dynamic": "^17.1.0",
    "@angular/platform-server": "^17.1.0",
    "@angular/router": "^17.1.0",
    "@angular/ssr": "^17.1.2",
    "@picocss/pico": "^1.5.11",
    "express": "^4.18.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.2",
    "@angular/cli": "^17.1.2",
    "@angular/compiler-cli": "^17.1.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "copyfiles": "^2.4.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jasmine-core": "~5.1.0",
    "json-server": "^0.17.4",
    "json-server-auth": "^2.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "npm-check-updates": "^16.14.14",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "typescript": "~5.3.2"
  }
}
