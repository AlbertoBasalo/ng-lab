  "scripts": {
    "ng": "ng",
    "chrome": "start chrome -incognito -auto-open-devtools-for-tabs http://localhost:4200",
    "start": "npm run chrome &&  ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test ",
    "serve:ssr:ActivityBookings": "node dist/activity-bookings/server/server.mjs",
    "api": "json-server-auth ../db/prod/d.json -r ../db/r.json",
    "api:reset": "copyfiles -f ../db/reset/d.json ../db/prod && npm run api",
    "api:seed": "copyfiles -f ../db/seed/d.json ../db/prod && npm run api",
    "update": "ncu -u",
    "lint": "ng lint"
  },
  "private": true,
